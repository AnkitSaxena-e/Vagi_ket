<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/backbackbackback"
    android:layout_height="match_parent"
    tools:context=".Fragments.AdminSettingFragment">

    <RelativeLayout android:id="@+id/ujujujuj" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:textSize="15dp" android:textStyle="normal|bold|italic" android:textColor="@color/white" android:id="@+id/llllll" android:padding="10dp" android:layout_width="wrap_content" android:layout_height="40dp" android:text="Users"/>
        <RelativeLayout android:id="@+id/jjjjjj" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/llllll">
            <androidx.appcompat.widget.SearchView android:id="@+id/numberSearchAdmin_PPPP" android:background="@drawable/loginbutton" android:layout_width="match_parent" android:layout_height="50dp"/>
            <TextView android:textSize="18dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:text="Number" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:layout_below="@+id/jjjjjj">
            <androidx.appcompat.widget.SearchView android:id="@+id/nameSearchAdmin_PPPP" android:background="@drawable/loginbutton" android:layout_width="match_parent" android:layout_height="50dp"/>
            <TextView android:textSize="18dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:text="Name" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        </RelativeLayout>
    </RelativeLayout>
    <androidx.recyclerview.widget.RecyclerView android:orientation="vertical" android:id="@+id/AdminUserData_PPPPpp" android:padding="2dp" android:scrollbars="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="5dp" android:layout_marginTop="5dp" android:layout_below="@+id/ujujujuj" android:columnOrderPreserved="false" android:alignmentMode="alignMargins"/>


</RelativeLayout>